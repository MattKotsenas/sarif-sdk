{
  "$schema": "https://schemastore.azurewebsites.net/schemas/json/sarif-2.1.0-rtm.5.json",
  "version": "2.1.0",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "FailureLevelRoundtrippingTester",
          "rules": [
            {
              "id": "HasExplicitDefaultLevel",
              "defaultConfiguration": { "level": "warning" }
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "HasExplicitDefaultLevel/ShouldBeNone",
          "level": "none",
          "message": { "text": "Level should be 'none' as it is directly expressed on the result. This is a rule trace message." }
        },
        {
          "ruleId": "HasExplicitDefaultLevel/ShouldBeNone",
          "kind": "notApplicable",
          "message": { "text": "Level should be 'none' because kind is not 'fail' and level is missing (so 'none' is the default)." }
        },
        {
          "ruleId": "HasExplicitDefaultLevel/ShouldBeNote",
          "level": "note",
          "message": { "text": "Level should be 'note' as it is directly expressed on the result." }
        },
        {
          "ruleId": "HasExplicitDefaultLevel/ShouldBeWarning",
          "message": { "text": "Level is 'warning' both because kind is implictly 'fail' (so 'warning' is the default) and because 'warning' is the default configuration level." }
        },
        {
          "ruleId": "HasExplicitDefaultLevel/ShouldBeError",
          "level": "error",
          "message": { "text": "Level should be 'error' as it is directly expressed on the result." }
        }
      ],
      "columnKind": "utf16CodeUnits"
    }
  ]
}